{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            // "request": "attach",
            "name": "Launch Chrome against localhost",
            "runtimeArgs": [
                "--load-extension=${workspaceFolder}/dist"
            ],
            // "port": 9229,
